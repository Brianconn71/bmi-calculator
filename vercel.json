{
    "version": 2,
    "builds": [
      {
        "src": "BMICalculator/BMICalculator/wsgi.py",
        "use": "@vercel/python",
        "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10"
      }
      },
      {
        "src": "bmicalculator-frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/bmicalculator-frontend/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "BMICalculator/BMICalculator/wsgi.py"
      }
    ]
  }